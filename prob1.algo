ALGORITHM sum_array
VAR
    set1,set2 : ARRAY_OF INTEGER;
    sum,i,j:INTEGER := 0;
BEGIN
    for i from 0 to set1.length-1 step 1 do 
    BOOLEAN:=false;
     for j from 0 to set2.length-1 step 1 do 
     if (set1[i] = set2[j]) then
     break;
     end if
     end for
     if (BOOLEAN = false) then
     sum:=sum+set1[i]
     end if 
     end for
     

END